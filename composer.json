{
    "name": "davibennun/laravel-push-notification",
    "description": "Laravel package to send push notifications to mobile devices (apns, gcm)",
     "keywords": ["apns","gcm","push","notification", "laravel"],
    "authors": [
        {
            "name": "Neverdull",
            "email": "info@neverdull.nl"
        }
    ],
    repositories": [
    {
        "type":"package",
        "package": {
          "name": "nvrdll/NotificationPusher",
          "version":"1.0",
          "source": {
              "url": "https://github.com/nvrdll/NotificationPusher",
              "type": "git",
              "reference":"1.0"
            }
        }
    }
],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "nvrdll/NotificationPusher": "1.0"
    },
    "autoload": {
        "psr-0": {
            "nvrdll\\LaravelPushNotification": "src/"
        }
    },
    "minimum-stability": "dev"
}
